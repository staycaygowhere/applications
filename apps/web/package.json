		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.1.4",
		"vitest": "^0.29.1"
	},
	"type": "module",
	"packageManager": "npm@9.6.2",
	"dependencies": {
		"@auth/core": "^0.5.1",
		"@auth/sveltekit": "^0.3.0",
		"@shoelace-style/shoelace": "^2.1.0",
		"amazon-cognito-identity-js": "^6.2.0",
		"jose": "^4.13.1",
		"sk-auth": "^0.4.1"
